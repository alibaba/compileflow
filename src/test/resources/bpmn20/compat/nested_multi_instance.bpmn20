<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns:cf="http://compileflow.alibaba.com" xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             targetNamespace="http://example.com/compileflow">
  <process id="nestedMultiInstanceTest" name="Nested MultiInstance Test" isExecutable="true">
    <extensionElements>
      <cf:var name="pList" description="主循环" dataType="java.util.List&lt;java.lang.String&gt;" inOutType="param"/>
      <cf:var name="subList" description="子循环" dataType="java.util.List&lt;java.lang.Integer&gt;" inOutType="param"/>
    </extensionElements>
    <startEvent id="start" name="Start"/>
    <sequenceFlow id="flow1" sourceRef="start" targetRef="outerLoop"/>
    <subProcess id="outerLoop" name="Outer Loop">
      <multiInstanceLoopCharacteristics cf:collection="pList" cf:elementVar="p" cf:indexVar="i" cf:elementVarClass="java.lang.String"/>
      <startEvent id="subStart" name="Sub Start"/>
      <sequenceFlow id="subFlow1" sourceRef="subStart" targetRef="innerLoop"/>
      <subProcess id="innerLoop" name="Inner Loop">
        <multiInstanceLoopCharacteristics cf:collection="subList" cf:elementVar="q" cf:indexVar="j" cf:elementVarClass="java.lang.Integer"/>
        <startEvent id="innerSubStart" name="Inner Sub Start"/>
        <sequenceFlow id="innerFlow1" sourceRef="innerSubStart" targetRef="echoTask"/>
        <serviceTask id="echoTask" name="Echo Task" cf:type="java" cf:class="com.alibaba.compileflow.engine.process.mock.MockJavaClazz" cf:method="echo">
          <extensionElements>
            <cf:var name="p1" dataType="java.lang.String" contextVarName="p" inOutType="param"/>
            <cf:var name="p2" dataType="java.lang.Integer" contextVarName="q" inOutType="param"/>
          </extensionElements>
        </serviceTask>
        <sequenceFlow id="innerFlow2" sourceRef="echoTask" targetRef="innerSubEnd"/>
        <endEvent id="innerSubEnd" name="Inner Sub End"/>
      </subProcess>
      <sequenceFlow id="subFlow2" sourceRef="innerLoop" targetRef="subEnd"/>
      <endEvent id="subEnd" name="Sub End"/>
    </subProcess>
    <sequenceFlow id="flow2" sourceRef="outerLoop" targetRef="end"/>
    <endEvent id="end" name="End"/>
  </process>
</definitions> 